{
  "version": 3,
  "extra_config": {
    "security/cors": {
      "allow_origins": ["*"],
      "allow_methods": ["GET", "HEAD", "POST", "PUT", "PATCH", "DELETE"],
      "expose_headers": ["Content-Length", "Content-Type"],
      "allow_headers": [
        "Accept-Language",
        "Content-Type",
        "Authorization",
        "Accept",
        "Origin"
      ],
      "max_age": "12h",
      "allow_credentials": false,
      "debug": false
    }
  },
  "endpoints": [
    {
      "endpoint": "/tweet-ids",
      "method": "POST",
      "extra_config": {
        "auth/validator": {
          "alg": "RS256",
          "audience": ["https://dev-p1zuevhv7lzho4wp.eu.auth0.com/api/v2/"],
          "jwk_url": "https://dev-p1zuevhv7lzho4wp.eu.auth0.com/.well-known/jwks.json"
        }
      },
      "backend": [
        {
          "host": "http://localhost:3001",
          "method": "POST",
          "url_pattern": "/tweet-ids",
          "is_collection": true
        }
      ]
    },
    {
      "endpoint": "//getByUserId/{userId}",
      "method": "GET",
      "backend": [
        {
          "host": "http://localhost:3001",
          "method": "GET",
          "url_pattern": "/getByUserId/{userId}",
          "is_collection": true
        }
      ]
    },
    {
      "endpoint": "/tweet",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3001",
          "method": "POST",
          "url_pattern": "/"
        }
      ]
    },
    {
      "endpoint": "/tweet/follow",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3001",
          "method": "POST",
          "url_pattern": "/follow"
        }
      ]
    },
    {
      "endpoint": "/tweet/unfollow",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3001",
          "method": "POST",
          "url_pattern": "/unfollow"
        }
      ]
    },
    {
      "endpoint": "/user",
      "method": "GET",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "GET",
          "url_pattern": "/",
          "is_collection": true
        }
      ]
    },
    {
      "endpoint": "/user",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "POST",
          "url_pattern": "/"
        }
      ]
    },
    {
      "endpoint": "/user/{id}",
      "method": "GET",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "GET",
          "url_pattern": "/{id}"
        }
      ]
    },
    {
      "endpoint": "/user/check/{username}",
      "method": "GET",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "GET",
          "url_pattern": "/check/{username}"
        }
      ]
    },
    {
      "endpoint": "/user/{id}/follow/{followUserId}",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "POST",
          "url_pattern": "/{id}/follow/{followUserId}"
        }
      ]
    },
    {
      "endpoint": "/user/{id}/unfollow/{unfollowUserId}",
      "method": "POST",
      "backend": [
        {
          "host": "http://localhost:3002",
          "method": "POST",
          "url_pattern": "/{id}/unfollow/{unfollowUserId}"
        }
      ]
    },
    {
      "endpoint": "/timeline/{id}",
      "method": "GET",
      "backend": [
        {
          "host": "http://localhost:3003",
          "method": "GET",
          "url_pattern": "/{id}"
        }
      ]
    }
  ]
}
